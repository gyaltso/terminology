          name="de.itemis.tooling.terminology.exporters.feature.feature.group"/>
      <sourceLocator
          rootFolder="${terminology.clone.location}"
          locateNestedProjects="true"/>
    </targlet>
  </setupTask>
  <setupTask
      xsi:type="setup.workingsets:WorkingSetTask">
    <workingSet
        name="Terminology">
      <predicate
          xsi:type="predicates:OrPredicate">
        <operand
            xsi:type="predicates:NamePredicate"
            pattern=".*tooling\.terminology"/>
        <operand
            xsi:type="predicates:NamePredicate"
            pattern=".*tooling\.terminology\.ui"/>
      </predicate>
    </workingSet>
    <workingSet
        name="Exporters">
      <predicate
          xsi:type="predicates:NamePredicate"
          pattern=".*export"/>
    </workingSet>
    <workingSet
        name="Websearch">
      <predicate
          xsi:type="predicates:NamePredicate"
          pattern="sample.*"/>
      <predicate
          xsi:type="predicates:NamePredicate"
          pattern=".*websearch.*"/>
    </workingSet>
    <workingSet
        name="Distro">
      <predicate
          xsi:type="workingsets:ExclusionPredicate"
          excludedWorkingSet="//@setupTasks.2/@workingSets.1 //@setupTasks.2/@workingSets.0 //@setupTasks.2/@workingSets.2"/>
    </workingSet>
  </setupTask>
  <setupTask
      xsi:type="setup:VariableTask"
      id="terminology.git.clone.variable"
      name="terminology.clone.location"
      value="${installation.location}/git"
      defaultValue=""/>
  <setupTask
      xsi:type="git:GitCloneTask"
      id="git.clone.xturtle"
      location="${terminology.clone.location}"
      remoteURI="https://github.com/nittka/Terminology.git">
    <description>Xturtle</description>
  </setupTask>
